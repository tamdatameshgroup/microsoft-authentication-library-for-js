{
  "name": "@azure/msal-node-extensions",
  "version": "1.0.0-alpha.35",
  "repository": {
    "type": "git",
    "url": "https://github.com/AzureAD/microsoft-authentication-library-for-js.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "gypfile": true,
  "keywords": [
    "js",
    "ts",
    "node",
    "AAD",
    "msal",
    "extensions"
  ],
  "files": [
    "dist",
    "src",
    "binding.gyp"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "node-gyp rebuild && rollup -c --strictDeprecations --bundleConfigAsCjs",
    "build:common": "cd ../../lib/msal-common && npm run build",
    "build:all": "npm run build:common && npm run build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "cd ../../ && npm run lint:node:extensions",
    "lint:fix": "npm run lint -- -- --fix",
    "prepack": "npm run build:all"
  },
  "author": {
    "name": "Microsoft",
    "email": "nugetaad@microsoft.com",
    "url": "https://www.microsoft.com"
  },
  "module": "dist/msal-node-extensions.esm.js",
  "beachball": {
    "disallowedChangeTypes": [
      "patch",
      "minor",
      "major"
    ]
  },
  "dependencies": {
    "@azure/msal-common": "14.0.0-alpha.1",
    "@azure/msal-node-runtime": "^0.13.6-alpha.0",
    "keytar": "^7.8.0",
    "node-addon-api": "^6.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.8.3",
    "jest": "^29.5.0",
    "rollup": "^3.20.2",
    "ts-jest": "^29.1.0",
    "tslib": "^2.0.0",
    "typescript": "^4.9.5"
  }
}
